<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="jt-web"/>

    <dubbo:registry address="zookeeper://192.168.163.5:2181?backup=192.168.163.6:2181,192.168.163.7:2181" />
    <dubbo:reference check="false" id="dubboUserService" interface="com.jt.dubbo.sso.DubboUserService" timeout="10000"/>
</beans>
